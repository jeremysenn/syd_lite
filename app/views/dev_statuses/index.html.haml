%table.table.table-striped.table-hover
  %thead
    %tr
      %th= "Date"
      %th= "Status"
      %th= "Raw Status"

  %tbody
    - @dev_statuses.each do |dev_status|
      %tr
        %td= dev_status.date_time.strftime("%-m/%-d/%y %l:%M%p")
        %td= dev_status.status
        %td= dev_status.raw_status

= paginate @dev_statuses, :param_name => "dev_statuses_page"
            